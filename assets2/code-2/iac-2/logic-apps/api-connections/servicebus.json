{
    "$schema": "https://schema.management.azure.com/schemas/2019-04-01/deploymentTemplate.json#",
    "contentVersion": "1.0.0.0",
    "parameters": {
        "full": {
            "type": "object"
        }
    },
    "variables": {},
    "resources": [
        {
            "type": "Microsoft.Web/connections",
            "apiVersion": "2016-06-01",
            "name": "[parameters('full').logicApps.apiConnections.servicebus]",
            "location": "[parameters('full').general.location]",
            "properties": {
                "parameterValues": {
                    "connectionString": "[listKeys(resourceId('Microsoft.ServiceBus/namespaces/authorizationRules', parameters('full').serviceBus.name, 'RootManageSharedAccessKey'), '2017-04-01').primaryConnectionString]"
                },
                "api": {
                    "id": "[subscriptionResourceId('Microsoft.Web/locations/managedApis', parameters('full').general.location, 'servicebus')]"
                }
            }
        }
    ]
}