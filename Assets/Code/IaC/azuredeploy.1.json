                },
                "parametersLink": {
                    "uri": "[concat(parameters('templatesBaseUri'), '/azuredeploy.parameters.json')]",
                    "contentVersion": "1.0.0.0"
                }
            }
        },
        {
            "type": "Microsoft.Resources/deployments",
            "apiVersion": "2019-10-01",
            "name": "storageAccount",
            "properties": {
                "mode": "Incremental",
                "templateLink": {
                    "uri": "[concat(parameters('templatesBaseUri'), '/Storage & Messaging/storage-account.json')]",
                    "contentVersion": "1.0.0.0"
                },
                "parametersLink": {
                    "uri": "[concat(parameters('templatesBaseUri'), '/azuredeploy.parameters.json')]",
                    "contentVersion": "1.0.0.0"
                }
            }
        },
        {
            "type": "Microsoft.Resources/deployments",
            "apiVersion": "2019-10-01",
            "name": "serviceBus",
