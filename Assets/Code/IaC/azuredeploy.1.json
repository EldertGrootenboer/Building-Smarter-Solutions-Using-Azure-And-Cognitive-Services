{
    "$schema": "https://schema.management.azure.com/schemas/2019-04-01/deploymentTemplate.json#",
    "contentVersion": "1.0.0.0",
    "parameters": {
        "rdwApiSubscriptionKey": {
            "type": "securestring"
        },
        "servicePrincipalPasswordEventGridValue": {
            "type": "securestring"
        },
        "full": {
            "type": "object"
        },
        "templatesBaseUri": {
            "type": "string",
            "defaultValue": "https://raw.githubusercontent.com/EldertGrootenboer/Sessions/master/Building%20Smarter%20Solutions%20Using%20Azure%20and%20Cognitive%20Services/Code/IaC"
        }
    },
    "variables": {},
    "resources": [
        {
            "type": "Microsoft.Resources/deployments",
            "apiVersion": "2019-10-01",
            "name": "keyVault",
            "properties": {
                "mode": "Incremental",
                "templateLink": {
                    "uri": "[concat(parameters('templatesBaseUri'), '/Security/key-vault.json')]",
                    "contentVersion": "1.0.0.0"
                },
                "parameters": {
                    "rdwApiSubscriptionKey": {
                        "value": "[parameters('rdwApiSubscriptionKey')]"
                    },
                    "servicePrincipalPasswordEventGridValue": {
                        "value": "[parameters('servicePrincipalPasswordEventGridValue')]"
                    },
                    "full": {
                        "value": "[parameters('full')]"
                    }
                }
            }
        },
        {
            "type": "Microsoft.Resources/deployments",
            "apiVersion": "2019-10-01",
            "name": "logAnalytics",
            "properties": {
                "mode": "Incremental",
                "templateLink": {
                    "uri": "[concat(parameters('templatesBaseUri'), '/Monitoring/log-analytics.json')]",
                    "contentVersion": "1.0.0.0"
                },
                "parametersLink": {
                    "uri": "[concat(parameters('templatesBaseUri'), '/azuredeploy.parameters.json')]",
                    "contentVersion": "1.0.0.0"
                }
            }
        },
        {
            "type": "Microsoft.Resources/deployments",
            "apiVersion": "2019-10-01",
            "name": "applicationInsights",
            "properties": {
                "mode": "Incremental",
                "templateLink": {
                    "uri": "[concat(parameters('templatesBaseUri'), '/Monitoring/application-insights.json')]",
