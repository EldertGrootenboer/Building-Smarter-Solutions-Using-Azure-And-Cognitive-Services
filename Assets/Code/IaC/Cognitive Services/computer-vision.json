{
    "$schema": "https://schema.management.azure.com/schemas/2019-04-01/deploymentTemplate.json#",
    "contentVersion": "1.0.0.0",
    "parameters": {
        "full": {
            "type": "object"
        }
    },
    "variables": {},
    "resources": [
        {
            "type": "Microsoft.CognitiveServices/accounts",
            "apiVersion": "2017-04-18",
            "name": "[parameters('full').cognitiveServices.computerVision.name]",
            "location": "[parameters('full').general.location]",
            "tags": "[parameters('full').general.tags]",
            "sku": {
                "name": "[parameters('full').cognitiveServices.computerVision.sku]"
            },
            "kind": "ComputerVision",
            "properties": {
                "customSubDomainName": "[parameters('full').cognitiveServices.computerVision.name]",
                "publicNetworkAccess": "Enabled"
            }
        }
    ]
}